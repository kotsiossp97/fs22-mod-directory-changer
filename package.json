{
  "name": "fs22-mod-directory-changer",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "electron-is-dev": "^2.0.0",
    "electron-settings": "^4.0.2",
    "material-react-table": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"npm start BROWSER=none\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never",
    "publish": "electron-builder build --mac --win --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0"
  },
  "author": {
    "name": "Konstantinos Andreou",
    "email": "kotsiossp@gmail.com",
    "url": "https://github.com/kotsiossp97"
  },
  "build": {
    "appId": "com.kotsiossp97.fs22-mod-directory-changer",
    "productName": "FS22 Mod Directory Changer",
    "copyright": "Copyright Â© 2023 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "build"
    }
  }
}
